{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "extends": [
    ],
    "rules": {
        "no-var": "warn",
        "prefer-const": "warn",
        "no-unused-vars": "warn",
        "eqeqeq": "error",
        "no-eval": "error"
    }
}
